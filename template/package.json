{
  "name": "template-starter-design-system",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:mock": "vite --mode mock",
    "build": "vite build",
    "preview": "vite preview",
    "test": "node scripts/test.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "node scripts/playwright.js test",
    "test:e2e:watch": "node scripts/playwright.js test --watch",
    "test:e2e:ui": "node scripts/playwright.js test --ui",
    "test:e2e:ui:watch": "node scripts/playwright.js test --watch --ui",
    "lint": "node scripts/lint.js",
    "lint:eslint": "eslint \"{src,tests,mocks}/**/*.{js,jsx,mjs,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"{src,tests,mocks}/**/*.{js,jsx,mjs,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "lint:stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint:prettier": "prettier \"{src,tests,mocks}/**/*.!(js|jsx|mjs|ts|tsx|css|scss)\" --check --ignore-unknown",
    "lint:prettier:fix": "prettier \"{src,tests,mocks}/**/*.!(js|jsx|mjs|ts|tsx|css|scss)\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen",
    "package:check": "npm exec --yes package-lock-utd@1.1.3",
    "postinstall": "msw init"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-router": "^7.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@msw/playwright": "^0.4.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.12",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.8.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "monocart-coverage-reports": "^2.12.9",
    "msw": "^2.11.3",
    "nodemon": "^3.1.10",
    "playwright-bdd": "^8.4.1",
    "prettier": "^3.6.2",
    "prettier-plugin-gherkin": "^3.1.2",
    "rimraf": "^6.0.1",
    "stylelint": "^16.24.0",
    "stylelint-config-pretty-order": "^0.7.0",
    "stylelint-config-recommended-scss": "^16.0.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": "eslint --fix",
    "*.{ts,tsx}": "tsc-files --noEmit",
    "*.{scss,css}": "stylelint --fix",
    "*.!(js|jsx|mjs|ts|tsx|scss|css)": "prettier --write --ignore-unknown",
    "(package|package-lock).json": "npm run package:check"
  },
  "volta": {
    "node": "22.19.0",
    "npm": "11.6.0"
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
